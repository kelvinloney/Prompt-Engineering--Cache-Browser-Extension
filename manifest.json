{
  "manifest_version": 3,
  "name": "Cache Chrome Extension",
  "version": "1.0.0",
  "description": "A Chrome extension that clears browser cache.",
  "action": {
    "default_popup": "src/popup.html"
  },
  "background": {
    "service_worker": "src/background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["src/content.js"]
    }
  ],
  "permissions": ["browsingData", "storage", "tabs"]
}
